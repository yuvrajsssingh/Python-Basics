for i in range(1,8):
    
    for j in range(1,8):
        a=j
        a=4
        if j==1 or i==j or i==7:
            print("*",end=" ")
        else:
            print(" ",end=" ")
    print() 

for i in range(1,8):
    for j in range(1,8):
        if j<=i and j<=8-i:
            print("*",end=" ")
        else:
            print(" ",end=" ")
    print()  

a=2
for i in range(1,4):
    for j in range(1,5):
        if j<=5-i:
            print(a,end=" ")
            a*=2
        
        else:
            print(" ",end=" ")
    print()


a=4
for i in range(1,4):
    for j in range(1,4):
        if j<=i:
            print(a,end=" ")
            a+=2
        else:
            print(" ",end=" ")
    print()



a=0
for i in range(1,5):
    for j in range(1,5):
        if j<=i:
            print(chr(100+a),end=" ")
            a+=1
        
        else:
            print(" ",end=" ")
    print() 



